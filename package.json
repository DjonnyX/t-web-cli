{
  "name": "t-web-cli",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run lint && npm-run-all --parallel test-watch serve-dev",
    "build": "npm run lint && npm run test && npm run assemble-release",
    "lint": "./node_modules/.bin/tslint -c tslint.json src/**/*.ts",
    "lint-fix": "npm run lint --fix",
    "assemble-release": "./node_modules/.bin/webpack --config ./webpack.prod.js --env.NODE_ENV=production --progress",
    "serve-dev": "./node_modules/.bin/webpack-dev-server --open --colors --hot --inline --config ./webpack.dev.js --env.NODE_ENV=development --progress",
    "test": "./node_modules/.bin/mocha -r ts-node/register src/**/*.ts tests/**/*.spec.ts",
    "test-watch": "npm run test -- --watch"
  },
  "author": {
    "name": "Eugene Grebennikov",
    "email": "djonnyx@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tslint": "5.20.0",
    "typescript": "^3.6.4",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-livereload-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  }
}
